<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.exam.Quiz4Fragment">

    <TextView
        android:id="@+id/example_meaning"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/input_sentence_quiz4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="문장 퀴즈 여기 입력"
        app:layout_constraintEnd_toEndOf="@+id/example_meaning"
        app:layout_constraintStart_toStartOf="@+id/example_meaning"
        app:layout_constraintTop_toBottomOf="@+id/example_meaning" />

    <ImageView
        android:id="@+id/complete_quiz4"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:text="확인스"
        app:layout_constraintEnd_toEndOf="@+id/input_sentence_quiz4"
        app:layout_constraintStart_toStartOf="@+id/input_sentence_quiz4"
        app:layout_constraintTop_toBottomOf="@+id/input_sentence_quiz4"
        android:src="@drawable/quiz_next_button"/>

</androidx.constraintlayout.widget.ConstraintLayout>