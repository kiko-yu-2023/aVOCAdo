<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ui.exam.Quiz3Fragment">

    <WebView
        android:id="@+id/webview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/input_word_quiz3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="동영상 퀴즈 여기 입력"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/webview" />

    <ImageView
        android:id="@+id/complete_quiz3"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toEndOf="@+id/input_word_quiz3"
        app:layout_constraintStart_toStartOf="@+id/input_word_quiz3"
        app:layout_constraintTop_toBottomOf="@+id/input_word_quiz3"
        android:src="@drawable/quiz_next_button"/>

</androidx.constraintlayout.widget.ConstraintLayout>